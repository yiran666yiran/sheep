#include<stdio.h>
int main()
{
    int N,a,b,c;
    scanf("%d",&N);
    if(N<100||N>999){
        printf("Invalid Value.");
    }
    else{
        a = N/100;
        b = N%100/10;
        c = N%10;
        if(a*a*a+b*b*b+c*c*c == N){
            printf("Yes");
        }else{
        printf("No");
        }
    }
    return 0;
}
